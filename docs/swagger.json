{
    "swagger": "2.0",
    "info": {
        "description": "Poc para estudos de GO",
        "title": "Todo list",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/login": {
            "post": {
                "description": "Gerador de token de acesso",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "Login",
                "parameters": [
                    {
                        "description": "Requisição de login",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/controller.userLogin"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/controller.tokenResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "controller.tokenResponse": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string",
                    "example": "asdfasdfasdf"
                }
            }
        },
        "controller.userLogin": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string",
                    "example": "email"
                },
                "password": {
                    "type": "string",
                    "example": "s3cr3t3"
                }
            }
        }
    }
}